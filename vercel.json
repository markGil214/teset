{
  "rewrites": [
    {
      "source": "/ARScannerPage.html",
      "destination": "/public/ARScannerPage.html"
    },
    {
      "source": "/BrainViewer.html",
      "destination": "/public/BrainViewer.html"
    },
    {
      "source": "/KidneyViewer.html", 
      "destination": "/public/KidneyViewer.html"
    },
    {
      "source": "/LungsViewer.html",
      "destination": "/public/LungsViewer.html"
    },
    {
      "source": "/SlicedHeartViewer.html",
      "destination": "/public/SlicedHeartViewer.html"
    },
    {
      "source": "/((?!api|_next|_static|favicon.ico|public|ARScannerPage|BrainViewer|KidneyViewer|LungsViewer|SlicedHeartViewer).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|webp|glb|gltf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}